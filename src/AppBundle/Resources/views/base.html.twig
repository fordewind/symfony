<!DOCTYPE html>
<html ng-app="usersApp">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {% stylesheets
                '@MopaBootstrapBundle/Resources/public/bootstrap/dist/css/bootstrap.min.css'
                '@MopaBootstrapBundle/Resources/public/bootstrap/dist/css/bootstrap-theme.min.css'
                '@AppBundle/Resources/public/css/*' %}
            <link href="{{ asset_url }}" type="text/css" rel="stylesheet"/>
            {% endstylesheets %}
        {% endblock %}

        {% javascripts
            'bundles/sunraangular/js/angular/angular-1.3.3/angular.min.js'
            '@AppBundle/Resources/public/js/controllers/*'
        %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
